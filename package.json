{
  "name": "fly-away-node",
  "version": "1.0.0",
  "license": "MIT",
  "description": "drinks and flights? Not a bad combo",
  "author": {
    "name": "Christopher K."
  },
  "scripts": {
    "globalnpms": "sudo npm install -g pm2 nodemon",
    "server": "cd ./build/server && nodemon --delay 5 www.js",
    "server:nonstop": "cd ./build/server && pm2 start www.js",
    "watch:server": "tsc -p ./server -w",
    "watch:client": "ng build --watch",
    "watch:all": "concurrently --kill-others \"tsc -p ./server -w\" \"ng build --watch\" ",
    "build:server": "rm -rf build/server && tsc -p ./server",
    "build:client": "rm -rf build/client && ng build --aot",
    "build:all": "rm -rf build && tsc -p ./server && ng build --aot"
    },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.1",
    "@angular/cdk": "~13.3.7",
    "@angular/common": "13.3.1",
